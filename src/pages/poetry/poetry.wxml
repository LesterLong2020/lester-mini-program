<!--pages/poetry/poetry.wxml-->
<navigation title="{{poetry.title}}" show-home="{{false}}" />
<view class="container">
  <view class="poetry-time">{{poetry.time}}</view>
  <view class="poetry-list">
    <view class="poetry-item" wx:for="{{poetry.content || []}}" wx:key="*this">
      {{item}}
    </view>
  </view>
</view>
